{
   "swagger":"2.0",
   "info":{
      "description":"Inventory test",
      "version":"1.0.0",
      "title":"Becommerce Inventory"
   },
   "host":"localhost:8080",
   "basePath":"/",
   "tags":[
      {
         "name":"inventory-controller",
         "description":"Inventory Controller"
      }
   ],
   "paths":{
      "/inventories/default/items":{
         "get":{
            "tags":[
               "inventory-controller"
            ],
            "summary":"findItems",
            "operationId":"findItemsUsingGET",
            "produces":[
               "*/*"
            ],
            "parameters":[
               {
                  "name":"page",
                  "in":"query",
                  "description":"page",
                  "required":false,
                  "type":"integer",
                  "default":0,
                  "format":"int32"
               },
               {
                  "name":"size",
                  "in":"query",
                  "description":"size",
                  "required":false,
                  "type":"integer",
                  "default":15,
                  "format":"int32"
               }
            ],
            "responses":{
               "200":{
                  "description":"OK",
                  "schema":{
                     "type":"array",
                     "items":{
                        "$ref":"#/definitions/InventoryItemDTO"
                     }
                  }
               },
               "401":{
                  "description":"Unauthorized"
               },
               "403":{
                  "description":"Forbidden"
               },
               "404":{
                  "description":"Not Found"
               }
            },
            "deprecated":false
         },
         "post":{
            "tags":[
               "inventory-controller"
            ],
            "summary":"addItem",
            "operationId":"addItemUsingPOST",
            "consumes":[
               "application/json"
            ],
            "produces":[
               "*/*"
            ],
            "parameters":[
               {
                  "in":"body",
                  "name":"dto",
                  "description":"dto",
                  "required":true,
                  "schema":{
                     "$ref":"#/definitions/InventoryItemDTO"
                  }
               }
            ],
            "responses":{
               "200":{
                  "description":"OK",
                  "schema":{
                     "$ref":"#/definitions/InventoryItemDTO"
                  }
               },
               "201":{
                  "description":"Created"
               },
               "401":{
                  "description":"Unauthorized"
               },
               "403":{
                  "description":"Forbidden"
               },
               "404":{
                  "description":"Not Found"
               }
            },
            "deprecated":false
         }
      },
      "/inventories/default/items/{inventoryItemId}":{
         "get":{
            "tags":[
               "inventory-controller"
            ],
            "summary":"getItem",
            "operationId":"getItemUsingGET",
            "produces":[
               "*/*"
            ],
            "parameters":[
               {
                  "name":"inventoryItemId",
                  "in":"path",
                  "description":"inventoryItemId",
                  "required":true,
                  "type":"string"
               }
            ],
            "responses":{
               "200":{
                  "description":"OK",
                  "schema":{
                     "$ref":"#/definitions/InventoryItemDTO"
                  }
               },
               "401":{
                  "description":"Unauthorized"
               },
               "403":{
                  "description":"Forbidden"
               },
               "404":{
                  "description":"Not Found"
               }
            },
            "deprecated":false
         },
         "delete":{
            "tags":[
               "inventory-controller"
            ],
            "summary":"removeItem",
            "operationId":"removeItemUsingDELETE",
            "produces":[
               "*/*"
            ],
            "parameters":[
               {
                  "name":"inventoryItemId",
                  "in":"path",
                  "description":"inventoryItemId",
                  "required":true,
                  "type":"string"
               }
            ],
            "responses":{
               "200":{
                  "description":"OK"
               },
               "204":{
                  "description":"No Content"
               },
               "401":{
                  "description":"Unauthorized"
               },
               "403":{
                  "description":"Forbidden"
               }
            },
            "deprecated":false
         }
      },
      "/inventories/default/items/{inventoryItemId}/amount":{
         "put":{
            "tags":[
               "inventory-controller"
            ],
            "summary":"updateAmount",
            "operationId":"updateAmountUsingPUT",
            "consumes":[
               "application/json"
            ],
            "produces":[
               "*/*"
            ],
            "parameters":[
               {
                  "in":"body",
                  "name":"dto",
                  "description":"dto",
                  "required":true,
                  "schema":{
                     "$ref":"#/definitions/InventoryOperationDTO"
                  }
               },
               {
                  "name":"inventoryItemId",
                  "in":"path",
                  "description":"inventoryItemId",
                  "required":true,
                  "type":"string"
               }
            ],
            "responses":{
               "200":{
                  "description":"OK",
                  "schema":{
                     "$ref":"#/definitions/InventoryItemDTO"
                  }
               },
               "201":{
                  "description":"Created"
               },
               "401":{
                  "description":"Unauthorized"
               },
               "403":{
                  "description":"Forbidden"
               },
               "404":{
                  "description":"Not Found"
               }
            },
            "deprecated":false
         }
      }
   },
   "definitions":{
      "InventoryItemDTO":{
         "type":"object",
         "properties":{
            "amount":{
               "type":"number",
               "example":15.0
            },
            "productDescription":{
               "type":"string",
               "example":"The description of Example Product"
            },
            "productName":{
               "type":"string",
               "example":"Example Product"
            }
         },
         "title":"InventoryItemDTO"
      },
      "InventoryOperationDTO":{
         "type":"object",
         "properties":{
            "operation":{
               "type":"string",
               "enum":[
                  "ADD",
                  "SUBTRACT"
               ]
            },
            "value":{
               "type":"number",
               "example":1.5
            }
         },
         "title":"InventoryOperationDTO"
      }
   }
}